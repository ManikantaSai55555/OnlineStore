<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UsersDao</servlet-name>
  	<display-name>UsersDao</display-name>
  	<description></description>
  	<servlet-class>com.src.dao.UsersDao</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UsersServlet</servlet-name>
  	<display-name>UsersServlet</display-name>
  	<description></description>
  	<servlet-class>com.src.ctrl.UsersServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ItemServlet</servlet-name>
  	<display-name>ItemServlet</display-name>
  	<description></description>
  	<servlet-class>com.src.ctrl.ItemServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>OrdersServlet</servlet-name>
  	<display-name>OrdersServlet</display-name>
  	<description></description>
  	<servlet-class>com.src.ctrl.OrdersServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UsersDao</servlet-name>
  	<url-pattern>/usersdao</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UsersServlet</servlet-name>
  	<url-pattern>/usersservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ItemServlet</servlet-name>
  	<url-pattern>/itemservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>OrdersServlet</servlet-name>
  	<url-pattern>/ordersservlet</url-pattern>
  </servlet-mapping>
</web-app>
